    "vue": "^3.3.4",
    "vue-bpmn": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "^5.9.3",
    "vite": "^4.4.5",
    "vue-tsc": "^3.1.1"
  },
  "keywords": [
    "bpmn",
    "diagram",
