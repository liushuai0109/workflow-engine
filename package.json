{
  "name": "bpmn-explorer",
  "version": "1.0.0",
  "description": "A comprehensive BPMN diagram editor based on vue-bpmn with Vue 3 and TypeScript",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "start": "vite --host 0.0.0.0 --port 8003",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@bpmn-io/properties-panel": "^3.33.0",
    "bpmn-js": "^11.5.0",
